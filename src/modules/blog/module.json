{
    "name": "blog",
    "description": "Ein einfaches Blogsystem, das das Erstellen und Ansehen von Blogeinträgen mehrerer Blogs unterstützt. Jeder Text kann mithilfe von Markdown rudimentär formatiert werden.",
    "authors": ["KlnSdr"],
    "version": "1.0.1",
    "routes": [
        {
            "path": "/blog/:blogName/getArticles/:page",
            "type": "get"
        },
        {
            "path": "/blog/:blogName/view/:id",
            "type": "get"
        },
        {
            "path": "/blog/readArticle",
            "type": "get"
        },
        {
            "path": "/editor/:blogName",
            "type": "get"
        },
        {
            "path": "/editor/:blogName/saveArticle",
            "type": "post"
        },
        {
            "path": "/editor/getCurrentBlog",
            "type": "get"
        }
    ],
    "files": ["index", "editor"]
}
